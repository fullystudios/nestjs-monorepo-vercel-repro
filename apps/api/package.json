{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "docker:up": "docker compose up -d postgres redis keto --quiet-pull",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "pnpm run build && pnpm run docker:up && ./scripts/keto/wait-for-keto.sh localhost 4477 jest --config jest-e2e.json --runInBand --detectOpenHandles",
    "check-types": "tsc --noEmit",
    "migration:generate": "sh -lc 'name=${1:?Provide migration name}; typeorm-ts-node-commonjs migration:generate -d src/database/data-source.ts src/database/migrations/$name' --",
    "migration:create": "typeorm-ts-node-commonjs migration:create",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/database/data-source.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/database/data-source.ts",
    "migration:show": "typeorm-ts-node-commonjs migration:show -d src/database/data-source.ts",
    "bruno:generate": "tsx ./scripts/bruno/generate-bruno-collection.ts",
    "database:seed": "tsx ./scripts/seed/seed-with-progress.ts",
    "database:seed:fresh": "tsx ./scripts/seed/seed-with-progress.ts --fresh"
  },
  "dependencies": {
    "@keyv/redis": "^5.1.2",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@ory/client-fetch": "^1.22.1",
    "@redis/client": "^5.8.3",
    "axios": "^1.12.2",
    "cache-manager": "^7.2.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dayjs": "^1.11.13",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "keyv": "^5.5.3",
    "lodash": "^4.17.21",
    "passport": "^0.7.0",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "postmark": "^4.0.5",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.27"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@ory/keto-namespace-types": "0.13.0-alpha.0",
    "@types/express": "^5.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.17.20",
    "@types/node": "catalog:",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.2",
    "@usebruno/converters": "^0.13.1",
    "fs-extra": "^11.3.1",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "listr2": "^9.0.4",
    "nest-commander": "^3.20.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4",
    "typescript": "catalog:"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@api/(.*)$": "<rootDir>/$1"
    }
  }
}
